{
  "manifest_version": 3,
  "name": "Web-Scraper",
  "version": "1.0",
  "permissions": ["activeTab", "storage", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "scraper.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"  
      ],
      "js": ["content.js"]
    }
  ]
}

